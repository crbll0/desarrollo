<odoo>
  <data>
    
    <record id="wizard_cuadre_caja_form" model="ir.ui.view" >
        <field name="name">wizard_cuadre_caja_form</field>
        <field name="model">wizard.cuadre.caja</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Cuadre de Caja">
                <group>
                    <group>
                        <field name="date_from"/>
                    </group>
                    <group>
                        <field name="date_to"/>
                        <field name="journal_ids" widget="many2many_tags"/>
                        <field name="user_ids" widget="many2many_tags"/>
                    </group>
                </group>
                <footer>
                    <button name="get_report" string="Generar" type="object" class="oe_highlight"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <act_window id="action_cuadre_caja"
        name="Cuadre de Caja"
        res_model="wizard.cuadre.caja"
        view_mode="form"
        target="new"/>

    <menuitem action="action_cuadre_caja"
        id="menu_cuadre_caja"
        parent="account.menu_finance_entries"/>
  </data>
</odoo>
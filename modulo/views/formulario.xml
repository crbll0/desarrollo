        <record id="real_estate_form_diligence_tree" model="ir.ui.view">
            <field name="name">Formulario de debida diligencia</field>
            <field name="model">real.estate.custom.doc.formulario</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="lastname"/>
                    <field name="email"/>
                    <field name="doc_type"/>
                    <field name="id_doc"/>
                </tree>
            </field>
        </record>

